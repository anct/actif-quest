<h1 class="first">管理者一覧 <small>- Admins</small></h1>
<br>

<table class="table table-hover table-row-clickable">

  <tbody>
    <% @admins.each do |admin| %>
      <tr class="pointer-cursor" data-href="<%= admin_admin_path(admin) %>">
        <td><%= admin.name %></td>
        <td><%= admin.email %></td>
        <td><%= admin.sign_in_count %> sign in</td>
        <td><%= admin.last_sign_in_at %></td>
        <td><%= admin.remember_created_at %></td>
        <td>
          <%= admin_action_btn 'Destroy', admin_admin_path(admin), method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin', new_admin_admin_path %>
